/* pages/Interviewlist/Interviewlist.wxss */

page{background: #f3f4f5;font-size: 24rpx;}
.start-btn{background: #3eccb3;color: #fff;position: fixed;bottom: 0;left: 0;
  height: 88rpx;line-height: 88rpx;font-size: 32rpx;text-align: center;width: 100%;
}
.start-btn.iphonex-start{
  bottom:68rpx;
}
.top{position: relative;}
.list-item{padding: 30rpx;margin-bottom: 20rpx;background: #fff;}
.left{width: 690rpx;}
.title{
  font-size: 30rpx;
  color: #333;
  line-height: 40rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.left text{font-size: 24rpx;color: #999;line-height: 24rpx;padding-top: 10rpx;display: block;}
.right{text-align: right;color: #f86645; width: 210rpx;position: absolute;top:50%;right:0;
margin-top: -32rpx;}
.score{font-size: 36rpx;}
.score_name{padding: 0 16rpx;background: #e5e5e5;color: #999;border-radius: 54rpx;height: 54rpx;
line-height: 56rpx;display: inline-block;}
.bottom{margin-top: 20rpx;padding: 20rpx 0 0;border-top: 1px solid #eee;color: #f86645;text-align: right;}
.bottom text,.bottom navigator{display:inline-block;padding:5rpx 15rpx;border: 1px solid  #f86645; 
margin-left: 30rpx;border-radius:8rpx; }
.bottom{margin-top: 20rpx;padding: 20rpx 0 0;border-top: 1px solid #eee;color: #f86645;text-align: right;}

.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(0, 0, 0, .6)
}
.copycontent{
  width: 570rpx;
  background: #fff;
  border-radius: 8rpx;
  position: fixed;
  left:50rpx;
  top:30%;
  padding: 50rpx 40rpx;
}
.copy-t{
  font-size:30rpx;
  color: #333;
  padding-bottom: 50rpx;
  text-align: center;

}
.copy-c{
  border:1px solid #e5e5e5;
  padding: 50rpx 30rpx 40rpx;
  margin-bottom: 50rpx;
}
.copy-tip{
  font-size: 30rpx;
  color: #999;
  line-height: 30rpx;
  padding-bottom: 20rpx;
  display: block;
}
.copy-wxcode{
  display: inline-block;
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 30rpx;
  color: #333;
  font-size: 30rpx;
  width: 330rpx;
  border:1px solid #e5e5e5;
  margin-right: 20rpx;
}
.copy-btn{
  width: 120rpx;
  background: #f97c4b;
  color: #fff;
  line-height: 70rpx;
  height: 70rpx;
  text-align: center;
  font-size: 30rpx;
  float: right;
}
.KF{
  background: #3eccb3;
  color: #fff;  
}
.KF::after{
  border:none;
}
.close{
  width: 18rpx;
  height: 18rpx;
  position: absolute;
  right:20rpx;
  top:20rpx;

}
.tip{
  font-size: 32rpx;
  padding: 100rpx 0;
  text-align: center;
  color: #f86645;
}
.list{
  padding-bottom: 100rpx;
}